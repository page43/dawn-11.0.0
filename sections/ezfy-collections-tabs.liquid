<div class="EzfyCollection EzfyCollection--{{ layout }}">
  <div class="EzfyCollection-container wrapper container page-width">


    {% if subtitle_position == 'above' %}<p class="EzfyCollection-subtitle">{{ subtitle }}</p>{% endif %}
    <p class="EzfyCollection-title">{{ title }}</p>
    {% if subtitle_position == 'below' %}<p class="EzfyCollection-subtitle">{{ subtitle }}</p>{% endif %}

    
    <div class="EzfyCollection-options">
    <div class="EzfyCollection-options-wrapper">
    {% for block in section.blocks %}
      {% assign collection = collections[block.settings.collection] %}
      {% assign title = block.settings.title %}
    
      {% if collection == blank %}
          <a href="#" data-collection-title="||___null" class="EzfyCollection-option {% if forloop.index0 == 0 %} EzfyCollection-option--active {% endif %}">
           <span class="EzfyCollection-option-text">Empty</span>
         </a>
      {% else %}
         <a href="#" data-collection-title="{{ collection.handle | downcase }}" class="EzfyCollection-option {% if forloop.index0 == 0 %} EzfyCollection-option--active {% endif %}">
           <span class="EzfyCollection-option-text">{% if title == "" %}{{ collection.title }}{% else %}{{ title }}{% endif %}</span>
         </a>
      {% endif %}

      
        {% if tab_style == 'separator'%}
      <span class="EzfyCollection-option-separator">/</span>
          {% endif %}
    {% endfor %}
    </div>
  </div>
   


  <div {%  unless request.design_mode  %}style="opacity:0;overflow:hidden;" {% endunless %} data-id="{{ section.id }}" class="EzfyCollection-products">

 {% for block in section.blocks limit:1 %}
      {% assign collection = collections[block.settings.collection] %}

      {% for product in collection.products limit: 3 %}
            <div class="EzfyCollection-product">
              <div class="EzfyCollection-AspectRatio EzfyCollection-AspectRatio--{{ image_size }} EzfyCollection-AspectRatio-mobile--{{ image_size_mobile }}">
                <img class="EzfyCollection-product-image EzfyCollection-product-image--lazyload" 
                   src="{{ product.featured_image | img_url: loading_image }}" 
                    data-sizes="100w"
                     sizes="100vw"
                  alt="{{ product.featured_image.alt }}" />
              </div>
                    <p class="EzfyCollection-product-title">{{ product.title }}</p>
              <a class="EzfyCollection-product-clickable" href="{{ product.handle }}"></a>
            </div>
 
      {% endfor %}
     
    {% endfor %}
   

  </div>

    {% if dots_enable %}
<div class="EzfyCollection-dots-wrapper">
    <div class="EzfyCollection-dots EzfyCollection-dots--progress"></div>
      </div>
      {% endif %}
      </div>
 <div class="EzfyCollection-button-wrapper">
   {% unless button_text == "" %}
<a href="{{ button_link }}" class="EzfyCollection-button">{{ button_text }}</a>
     {% endunless %}
</div>
</div>


  

