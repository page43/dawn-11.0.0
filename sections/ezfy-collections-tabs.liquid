  function updateCurrentlySelectedTab($selected){

    const $previous = $this.querySelectorAll(`.EzfyCollection-option--active`);

    for (var each of $previous){
      each.classList.remove(`EzfyCollection-option--active`);
    }

    $selected.classList.add(`EzfyCollection-option--active`)
  }

  function addTransitioningClass(){


$this.classList.add(`EzfyCollection--transitioning`);
 
  }

    function removeTransitioningClass(){
$this.classList.remove(`EzfyCollection--transitioning`);

  }

  async function addProductsToCollection($button){
        if (!$button){
          $button = $this.querySelector(`.EzfyCollection-options [data-collection-title]:nth-child(1)`);
        }
    
        const title = $button.getAttribute(`data-collection-title`);
        const collection = window[`${collectionID}--collections`].filter(e => e.collection === title)[0];

        if (title === '||___null'){
          return;
        }
    
        if (!collection){
          throw new Error(`No collection with the handle ${title} found. What was found: ${collection}`)
        }

        const products = collection.products;

        if (!products){
          throw new Error(`No products for ${title} were found.`)
        }

        let html = ``;

        for (var [i, product] of products.entries()){
          html += `
                 <div class="EzfyCollection-product">
                 <a class="EzfyCollection-product-clickable" href="/products/${ product.handle }"></a>
                 <div
                 ${ (options.image_size === 'default' && !_isMobile()) && `style="padding-bottom: calc(100% / ${product.aspectRatio});"`}
                 ${ (options.image_size_mobile === 'default' && _isMobile()) && `style="padding-bottom: calc(100% / ${product.aspectRatio});"`}
                 class="EzfyCollection-AspectRatio EzfyCollection-AspectRatio--${options.image_size} EzfyCollection-AspectRatio-mobile--${options.image_size_mobile}">
                    <img class="EzfyCollection-product-image EzfyCollection-product-image--lazyload" 
                    src="${product.featuredImageLoading}"
                    data-srcset="${ product.featuredImageSrcSet }" 
                             data-sizes="100w"
                    sizes="100w"
               
                    alt="${ product.featuredImageAlt }" />

                    {% comment %}Second image on hover {% endcomment %} 
                    ${ (options.image_hover !== 'none') && (!product.hoverImage.includes(`assets/no-image`)) ? `
                    <img class="EzfyCollection-product-image EzfyCollection-product-image-hover EzfyCollection-product-image--lazyload" 
                    src="${product.hoverImage}"
                    data-src="${ product.hoverImageSrcSet }" 
                    data-sizes="100w"
                    sizes="100w"
                    alt="${ product.hoverImageAlt }" />
                    ` : ``}

                   
                    <div class="EzfyCollection-badge ${product.compareAtPrice && parseInt(product.compareAtPrice) > 0 ? 'EzfyCollection-badge--sale' : ""} ">
                        ${product.compareAtPrice && parseInt(product.compareAtPrice) > 0 ? `<span>${window['{{ section.id }}--variables'].sale_badge_text}</span>` : ""}
                    </div>
                   
                    <div class="EzfyCollection-badge ${product.isAvailable === false ? 'EzfyCollection-badge--sold-out' : ""} ">
                        <span>${product.isAvailable === false ? window['{{ section.id }}--variables'].sold_out_badge_text : ""}</span>
                    </div>

                 
                  </div>

                    <p class="EzfyCollection-product-title">${product.title}</p>
                    <div class="EzfyCollection-prices">
                    ${product.compareAtPrice >= 0 ? `<span class="EzfyCollection-compare-price">${product.formattedCompareAtPrice}</span>`:`` }
                    <div class="EzfyCollection-price-wrap" data-test="${product.priceVaries}" >
                    ${product.priceVaries && window['{{ section.id }}--variables'].enable_starting_from  ? `<span class="EzfyCollection-price-from">${window['{{ section.id }}--variables'].from_text}</span>` : ``}
                    <span class="EzfyCollection-price">${product.formattedPrice}</span>
                    </div>
                    
                    </div>
            </div>
          `
        }

  
        const $container = $this.querySelector(`.EzfyCollection-products`);

        $container.innerHTML = html;

 

        removeTransitioningClass()

        start();
  }

  function destroySlider(){
      if (options.layout == 'slider'){
        try{
          //alert(window[`${sectionID}--slider`])
          if (window[`${sectionID}--slider`]){
            window[`${sectionID}--slider`].destroy();
          }
        }catch(err){
          throw new Error(`Problem when destroying slider. ${err}`)
        } finally{
          window[`${sectionID}--slider`] = null;
        }
      }
  }

  function freezeCurrentHeight(){

    let height;
    
    height = $this.offsetHeight;
    $this.style.height = `${height}px`;
    $this.style.overflow = `hidden`;

    const $slider = $this.querySelector(`.EzfyCollection-products`);
    height = $slider.offsetHeight;
