  }

  function refreshCodeAtCustomizePage(e, isChangeWithinSection=false){
    if (Shopify.designMode) {

          
  
        const $collection = e.target;

        if (!$collection.classList.contains("ezfy-collections-section")) {
            return
        }

         console.log("changes!")
        console.log("refreshing", window[collectionID])
    
        const $variables = $collection.querySelector(`script[data-ezfy-custom-variables]`);
        const $script = $collection.querySelector(`script[data-collection-section-id]`);
    
        const variables = $variables.innerHTML;
        const script = $script.innerHTML;

        eval(variables);
        eval(script);

        window[collectionID].customizePageRefresh = true;

     if ( ([collectionID].hasOwnProperty("customizePageRefresh") && window[collectionID].customizePageRefresh) ){
           return;
          }

        setTimeout(() => {
          window[collectionID].destroySlider();
          window[collectionID].start();
          window[collectionID].addProductsToCollection(null);
          window[collectionID].updateCollectionProductsOnClick()
        },50)
    }
  }

  

  function isOverflown($el){
    const $container = $this.querySelector(`.EzfyCollection-container`);

    const containerWidth = $container.offsetWidth
    
        if ($el.offsetWidth > containerWidth){
            return true;
        }
    
        return false;
  }

  function adjustTabsIfOverflow(){
    const $tabs = $this.querySelector(`.EzfyCollection-options`);

    if (!$tabs){
      return;
    }
    

    if (isOverflown($tabs)){
      $tabs.classList.add(`EzfyCollection-options--overflow`)
    } else{
      //$tabs.classList.remove(`EzfyCollection-options--overflow`)
    }
  }

    function isAppleDevice() {
  return [
    'iPad Simulator',
    'iPhone Simulator',
    'iPod Simulator',
    'iPad',
    'iPhone',
    'iPod'
  ].includes(navigator.platform)
  // iPad on iOS 13 detection
  || (navigator.userAgent.includes("Mac") && "ontouchend" in document)
}
  
                 
  return {
    addProductsToCollection: addProductsToCollection,
    updateCollectionProductsOnClick: updateCollectionProductsOnClick,
    destroySlider: destroySlider,
    start: start,
    restart: restart,
    refreshCodeAtCustomizePage:refreshCodeAtCustomizePage,
    init: async function () {

      document.addEventListener("DOMContentLoaded", async function () {
      
          start()
        
        addProductsToCollection(null)
    
        updateCollectionProductsOnClick()
