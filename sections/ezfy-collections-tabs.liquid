<script data-collection-section-id="{{ section.id }}">

console.log(window[`{{ section.id }}--variables`]);

window[`{{ section.id }}--variables`].collections_quantity = parseInt(`{{ section.blocks.size }}`);


    
  
var __id;

try {
    __id = `{{ section.id }}`.trim();
} catch (err) {
    __id = document.currentScript.parentNode.querySelector(`[data-collection-section-id]`).getAttribute(`data-collection-section-id`);
}

var collectionID = `${__id}`;
  
window[collectionID] = window[collectionID] || {};
window[collectionID] = (function() {

var sectionID = `{{ section.id }}`.trim();
var collectionID = `${sectionID}`;

/* Get collections [START]
====================== */
  
window[`${collectionID}--collections`] = [];
   {% for block in section.blocks %}
var collection = `{{ block.settings.collection }}` 

   var products = [];

{% for product in block.settings.collection.products limit: products_limit %}


  var price = parseInt(`{{ product.price }}`);
  var compareAtPrice = parseInt(`{{ product.compare_at_price }}`)
  var compareAtPriceMin = parseInt(`{{ product.compare_at_price_min }}`);
  var compareAtPriceMax = parseInt(`{{ product.compare_at_price_max }}`);
  var priceMin = parseInt(`{{ product.price_min }}`)
  var priceMax = parseInt(`{{ product.price_max }}`)
  var formattedPrice = `{{ product.price | money}}`
  var formattedCompareAtPrice = `{{ product.compare_at_price | money }}`
  var formattedCompareAtPriceMax = `{{ product.compare_at_price_max | money}}`
  var formattedCompareAtPriceMin = `{{ product.compare_at_price_min | money}}`
  var formattedPriceMin = `{{ product.price_min | money }}`
  var formattedPriceMax = `{{ product.price_max | money }}`
  var priceVaries = `{{ product.price_varies }}` === 'true' ? true : false;


  /* Hover image */
  if (window[`${sectionID}--variables`].image_hover === 'second'){
    var hoverImage = `{{ product.images[1] | img_url: '800x' }}`;
    var hoverImageSrcSet = `{{ product.images[1] | img_url: '700x' }} 700w
                              {{ product.images[1] | img_url: '900x' }} 900w
                                 {{ product.images[1] | img_url: '1080x' }} 1080w,
                                 {{ product.images[1] | img_url: '1296x' }} 1296w,
                                 {{ product.images[1] | img_url: '1512x' }} 1512w,
                                 {{ product.images[1] | img_url: '1728x' }} 1728w,
                                 {{ product.images[1] | img_url: '2048x' }} 2048w,
                                 {{ product.images[1] | img_url: '2450x' }} 2450w,
                                 {{ product.images[1] | img_url: '2700x' }} 2700w,
                                 {{ product.images[1] | img_url: '3000x' }} 3000w`;
    var hoverImageLoading = `{{ product.images[1] | img_url: '1x' }}`;
    var hoverImageAlt = `{{ product.images[1].alt }}`;
  } else if (window[`${sectionID}--variables`].image_hover === 'last'){
    var hoverImage = `{{ product.images.last | img_url: '800x' }}`;
    var hoverImageSrcSet = `{{ product.images.last | img_url: '700x' }} 700w
  {{ product.images.last | img_url: '900x' }} 900w
                                 {{ product.images.last | img_url: '1080x' }} 1080w,
                                 {{ product.images.last | img_url: '1296x' }} 1296w,
                                 {{ product.images.last | img_url: '1512x' }} 1512w,
                                 {{ product.images.last | img_url: '1728x' }} 1728w,
                                 {{ product.images.last | img_url: '2048x' }} 2048w,
                                 {{ product.images.last | img_url: '2450x' }} 2450w,
                                 {{ product.images.last | img_url: '2700x' }} 2700w,
                                 {{ product.images.last | img_url: '3000x' }} 3000w`;
    var hoverImageLoading = `{{ product.images.last | img_url: '1x' }}`;
    var hoverImageAlt = `{{ product.images.last.alt }}`;
  }

  var featuredImage = `{{ product.featured_image | img_url: '800x' }}`
  var featuredImageAlt = `{{ product.featured_image.alt }}`;


  var featuredImageLoading = window["{{ section.id }}--variables"].loading_image === '1x' ? `{{ product.featured_image | img_url: '1x' }}` : `{{ product.featured_image | img_url: '300x' }}`;
  var featuredImageSrcSet = `{{ product.featured_image | img_url: '900x' }} 900w
                               {{ product.featured_image | img_url: '1080x' }} 1080w,
                               {{ product.featured_image | img_url: '1296x' }} 1296w,
                               {{ product.featured_image | img_url: '1512x' }} 1512w,
                               {{ product.featured_image | img_url: '1728x' }} 1728w,
                               {{ product.featured_image | img_url: '2048x' }} 2048w,
                               {{ product.featured_image | img_url: '2450x' }} 2450w,
                               {{ product.featured_image | img_url: '2700x' }} 2700w,
                               {{ product.featured_image | img_url: '3000x' }} 3000w`;
  
  var aspectRatio = `{{ product.featured_image.aspect_ratio }}`;
  var quantity = `{% for variant in product.variants %}{{variant.inventory_quantity}},{% endfor %}`.split(",").filter(e => e != "").reduce((a, b) => parseInt(a) + parseInt(b), 0);

  var product = {
    title: `{{ product.title }}`,
    handle: `{{ product.handle }}`,
    isAvailable: `{{ product.available }}` === 'true' ? true : false,
    price,
    compareAtPrice,
    compareAtPriceMin,
    compareAtPriceMax,
    priceMin,
    priceMax,
    featuredImage,
    featuredImageAlt,
    featuredImageLoading,
    featuredImageSrcSet,
    aspectRatio,
    formattedPrice,
    formattedCompareAtPrice,
    formattedCompareAtPriceMin,
    formattedCompareAtPriceMax,
    formattedPriceMin,
    formattedPriceMax,
    hoverImage,
    hoverImageSrcSet,
    priceVaries,
    quantity
  };
  
  
  products.push(product);
  {% endfor %}

  window[`${collectionID}--collections`].push({
    collection,
    products
  })
   


{% endfor %}


