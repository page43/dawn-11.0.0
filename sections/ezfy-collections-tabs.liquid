  function initTabsSlider(){
    const $wrapper = $this.querySelector(`.EzfyCollection-options:not(.flickity-enabled)`);

    if (!$wrapper){
      return;
    } 

    let tabs;

    try{
      tabs = $wrapper.querySelectorAll(`.EzfyCollection-option`).length;
  
      if (tabs <= 2){
        $wrapper.classList.add(`EzfyCollection-options--grid`);
        return;
      } else{
      $wrapper.classList.add(`EzfyCollection-options--slider`);
    }
    

   setTabsSliderWidth();

