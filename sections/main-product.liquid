  {% comment %}
  ## Generate thumbnails
  ====================================
  {% endcomment%} 
  
  {% if load_dawn_slider %}
  <ul id="EzfyDawnThemeThumbnails"  
      class="Thumbnails">
    
  
    <div class="Thumbnails--vertical--scroller Thumbnails--scroller">

    
    {% comment %}
    ==================================
    Displaying feeatured thumbnail (only 1). 
    ==================================
    {% endcomment %}
    
    {%- for media in product.media -%}   	
      {% if media.id == product.selected_or_first_available_variant.featured_media.id %}
    
    {% capture media_position %}
      {% if media.position == 1 %}
     	1
      {% else %}
      	{{ media.position }}
      {% endif %}
    {% endcapture %}
    
    {% assign first_thumbnail_position = media_position | strip | times: 1 %}
    
        <li class="Thumbnails-thumbnail" 
            id="Thumbnail-{{ media.id }}"
            data-media-position="{{ first_thumbnail_position }}"
            data-custom-variantiid="{{ product.variants[forloop.index0].id }}"
            data-image-id="{{ media.id }}" >
            <img class="Thumbnails--slider-image" 
                 src="{{ media.preview_image | img_url: '300x' }}"
                 loading="lazy"
                 />
        </li>   
      {% endif %}
      

    {%- endfor -%}
    

    {% comment %}
    ==================================
    Displaying all the other thumbnails.
    ==================================
    {% endcomment %}
    
    {% assign loop = 0 %}
    
    {%- for media in product.media -%}
      {% unless media.id == product.selected_or_first_available_variant.featured_media.id %} 
    
       	{% if media.media_type == 'image' %}
        	{% assign loop = loop | plus: 1 %}
      	{% endif %}
    
    
    	{% if loop == first_thumbnail_position %}
        	{% assign loop = loop | plus: 1 %}
    	{% endif %}
    
    	{% assign _loop = loop | minus: 1 %}

      <li class="Thumbnails-thumbnail" 
          id="Thumbnail-{{ media.id }}" 
          {% if media.media_type == 'image' %}
          data-media-position="{{ loop }}" 
          {% endif %}
          data-image-id="{{ media.id }}" >
          <img class="Thumbnails--slider-image" src="{{ media.preview_image | img_url: '300x' }}" />
      </li>
 
    {% endunless %}
    {%- endfor -%}
          

    </div>

  </ul>
  {% endif %}
